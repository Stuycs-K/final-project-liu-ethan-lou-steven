class Sprite {
  private float xcor;
  private float ycor;
  private boolean isAlive;
  private boolean isJumping;
  
  public Sprite(float x, float y) {
    xcor = x;
    ycor = y;
    isAlive = true;
    isJumping = false;
  }
  
  public float getX() {
    return xcor;
  }
  
  public float getY() {
    return ycor;
  }
  
  public boolean getAlive() {
    return isAlive;
  }
  
  public boolean isJumping() {
    return isJumping;
  }
  
  public void jump() {
  }
    
}
